<Window x:Class="Antelcat.Wpf.I18N.Windows.Tests.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tests="clr-namespace:Antelcat.Wpf.I18N.Windows.Tests"
        mc:Ignorable="d" d:DataContext="{d:DesignInstance tests:ViewModel}"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="{I18N Key={x:Static tests:LangKeys.English}}"/>
        </StackPanel>
    </Grid>
</Window>